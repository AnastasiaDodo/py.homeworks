def move_zeros_to_end(lst):
    non_zero_elements = [elem for elem in lst if elem != 0]
    zero_elements = [0] * (len(lst) - len(non_zero_elements))
    result = non_zero_elements + zero_elements
    return result

# Приклади використання:
example1 = [0, 1, 0, 12, 3]
result1 = move_zeros_to_end(example1)
print(result1)

example2 = [1, 0, 13, 0, 0, 0, 5]
result2 = move_zeros_to_end(example2)
print(result2)
